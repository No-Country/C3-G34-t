@model AuditApp.Models.FormAutoElevadores;
@using Microsoft.AspNetCore.Identity;
@inject SignInManager<UsuarioBase> SignInManager
@inject UserManager<UsuarioBase> UserManager

@{
    ViewData["Title"] = "Autoelevadores";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row">
    <div class=col-sm-12>
        <h3>Nueva Auditoría Autoelevadores</h3>
    </div>
</div>
<hr />
<div class="row">
    <div class="col-md-6 mx-auto">
        <form method="post" asp-action="CreateFAE">
            <input type="text" asp-for="AuditorGuId" value="@UserManager.GetUserAsync(User).Result.Id" class="invisible" />

            <div asp-validation-summary="ModelOnly" class="text-danger my-1"></div>
            <div class="form-group mt-0">
                <b>
                    <label class='control-label'>1. Fecha</label>
                </b>
                @*<label asp-for="Fecha" class="control-label"></label>*@
                <div class="input-group my-1">
                    <input asp-for="Fecha" class="form-control-plaintext bg-body" value="@DateTime.Now" type="text" readonly />
                </div>
                <span asp-validation-for="Fecha" class="text-danger my-1"></span>
            </div>
            <div class="form-group mt-1">
                <b>
                    <label class='control-label'>2. Razón Social / Empresa</label>
                </b>
                @*<label asp-for="PlantaId" class="control-label my-2">Razón Social / Empresa</label>*@
                <div class="input-group">
                    <span class="material-icons input-group-text">factory</span>
                    <select asp-for="PlantaId" class="form-select" name="PlantaId">
                        <option disabled selected>Elegir Planta</option>
                        @foreach (var p in (IEnumerable<Planta>)ViewData["Plantas"])
                        {
                            <option value="@p.Id">@p.Nombre</option>
                        }
                    </select>
                </div>
                <span asp-validation-for="PlantaId" class="text-danger my-1"></span>
            </div>

            <div class="form-group mt-4">
                <b>
                    <label class='control-label'>3. Elevador ID</label>
                </b>
                @*<label asp-for="ElevadorID" class="control-label my-2"></label>*@
                <input asp-for="ElevadorID" class="form-control" />
                <span asp-validation-for="ElevadorID" class="text-danger my-1"></span>
            </div>
            <div class="form-group mt-4">
                <b>
                    <label class='control-label'>4. Los autoelevadores deberán contener una placa identificatoria para el equipo y otra para el accesorio, la cual debería contener, en forma visible, indeleble, destacada y redactada en idioma español, la siguiente información</label>
                </b>

            </div>
            <div class="form-group mt-4">
                <label class='control-label'>4.1 La carga máxima admisible a transportar, conforme el Sistema Métrico Legal Argentino (SIMELA). </label>
                <div>
                    <div class="btn-group w-100 mt-1" role="group" aria-label="Basic radio toggle button group">
                        <input type="radio" class="btn-check" name="EtiquetaCargaMaxima" value="Ok" id="EtiquetaCargaMaximaok" autocomplete="off" checked>
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="EtiquetaCargaMaximaok">Ok</label>

                        <input type="radio" class="btn-check" name="EtiquetaCargaMaxima" value="No Ok" id="EtiquetaCargaMaximanook" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill mx-2 my-2 w-25 p-1" for="EtiquetaCargaMaximanook">No ok</label>

                        <input type="radio" class="btn-check" name="EtiquetaCargaMaxima" value="No Aplica" id="EtiquetaCargaMaximanoaplica" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="EtiquetaCargaMaximanoaplica">No aplica</label>
                    </div>
                    <span asp-validation-for="EtiquetaCargaMaxima" class="text-danger my-1"></span>
                </div>
                <span asp-validation-for="EtiquetaCargaMaxima" class="text-danger my-1"></span>
            </div>

            <div class="form-group mt-3">
                <label class='control-label'>4.2 La tabla de carga y/o curvas que permitan el cálculo de cargas máximas admisibles para distintas condiciones de uso, en el sistema métrico legal argentino</label>
                <div>
                    <div class="btn-group w-100 mt-1" role="group" aria-label="Basic radio toggle button group">
                        <input type="radio" class="btn-check" name="EtiquetasCurvas" value="Ok" id="EtiquetasCurvasok" autocomplete="off" checked>
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="EtiquetasCurvasok">Ok</label>

                        <input type="radio" class="btn-check" name="EtiquetasCurvas" value="No Ok" id="EtiquetasCurvasnook" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill mx-2 my-2 w-25 p-1" for="EtiquetasCurvasnook">No ok</label>

                        <input type="radio" class="btn-check" name="EtiquetasCurvas" value="No Aplica" id="EtiquetasCurvasnoaplica" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="EtiquetasCurvasnoaplica">No aplica</label>
                    </div>
                    <span asp-validation-for="EtiquetasCurvas" class="text-danger my-1"></span>
                </div>
                <span asp-validation-for="EtiquetasCurvas" class="text-danger my-1"></span>
            </div>


            <div class="form-group mt-3">
                <label class='control-label'>4.3 La identificación interna del autoelevador</label>
                <div>
                    <div class="btn-group w-100 mt-1" role="group" aria-label="Basic radio toggle button group">
                        <input type="radio" class="btn-check" name="Etiquetastringerna" value="Ok" id="Etiquetastringernaok" autocomplete="off" checked>
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="Etiquetastringernaok">Ok</label>

                        <input type="radio" class="btn-check" name="Etiquetastringerna" value="No Ok" id="Etiquetastringernanook" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill mx-2 my-2 w-25 p-1" for="Etiquetastringernanook">No ok</label>

                        <input type="radio" class="btn-check" name="Etiquetastringerna" value="No Aplica" id="Etiquetastringernanoaplica" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="Etiquetastringernanoaplica">No aplica</label>
                    </div>
                    <span asp-validation-for="Etiquetastringerna" class="text-danger my-1"></span>
                </div>
                <span asp-validation-for="Etiquetastringerna" class="text-danger my-1"></span>
            </div>

            <div class="form-group mt-3">
                <label class='control-label'>4.4. Las placas deberán cumplir con lo establecido por la Norma IRAM 8412-1, o la que en el futuro la modifique o sustituya</label>
                <div>
                    <div class="btn-group w-100 mt-1" role="group" aria-label="Basic radio toggle button group">
                        <input type="radio" class="btn-check" name="IRAM8412" value="Ok" id="IRAM8412ok" autocomplete="off" checked>
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="IRAM8412ok">Ok</label>

                        <input type="radio" class="btn-check" name="IRAM8412" value="No Ok" id="IRAM8412nook" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill mx-2 my-2 w-25 p-1" for="IRAM8412nook">No ok</label>

                        <input type="radio" class="btn-check" name="IRAM8412" value="No Aplica" id="IRAM8412noaplica" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="IRAM8412noaplica">No aplica</label>
                    </div>
                    <span asp-validation-for="IRAM8412" class="text-danger my-1"></span>
                </div>
                <span asp-validation-for="IRAM8412" class="text-danger my-1"></span>
            </div>

            <div class="form-group mt-4">
                <b>
                    <label class='control-label'>5. La cabina del autoelevador deberá cumplir con los siguientes requisitos</label>
                </b>

            </div>

            <div class="form-group mt-3">
                <label class='control-label'>5.1 Estructura resistente que proteja al operador contra caídas, proyección de objetos o por desplazamiento de la carga</label>
                <div>
                    <div class="btn-group w-100 mt-1" role="group" aria-label="Basic radio toggle button group">
                        <input type="radio" class="btn-check" name="EstructuraContraCaidas" value="Ok" id="EstructuraContraCaidasok" autocomplete="off" checked>
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="EstructuraContraCaidasok">Ok</label>

                        <input type="radio" class="btn-check" name="EstructuraContraCaidas" value="No Ok" id="EstructuraContraCaidasnook" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill mx-2 my-2 w-25 p-1" for="EstructuraContraCaidasnook">No ok</label>

                        <input type="radio" class="btn-check" name="EstructuraContraCaidas" value="No Aplica" id="EstructuraContraCaidasnoaplica" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="EstructuraContraCaidasnoaplica">No aplica</label>
                    </div>
                    <span asp-validation-for="EstructuraContraCaidas" class="text-danger my-1"></span>
                </div>
                <span asp-validation-for="EstructuraContraCaidas" class="text-danger my-1"></span>
            </div>

            <div class="form-group mt-3">
                <label class='control-label'>5.2 El autoelevador que deba operar con lluvia, nieve, agua nieve, etc., deberá contar con: cerramiento y un sistema de limpiaparabrisas, el aire en el interior de las cabinas con cerramientos, deberá cumplir los requisitos establecidos en la legislación vigente</label>
                <div>
                    <div class="btn-group w-100 mt-1" role="group" aria-label="Basic radio toggle button group">
                        <input type="radio" class="btn-check" name="ProteccionLLuvia" value="Ok" id="ProteccionLLuviaok" autocomplete="off" checked>
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="ProteccionLLuviaok">Ok</label>

                        <input type="radio" class="btn-check" name="ProteccionLLuvia" value="No Ok" id="ProteccionLLuvianook" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill mx-2 my-2 w-25 p-1" for="ProteccionLLuvianook">No ok</label>

                        <input type="radio" class="btn-check" name="ProteccionLLuvia" value="No Aplica" id="ProteccionLLuvianoaplica" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="ProteccionLLuvianoaplica">No aplica</label>
                    </div>
                    <span asp-validation-for="ProteccionLLuvia" class="text-danger my-1"></span>
                </div>
                <span asp-validation-for="ProteccionLLuvia" class="text-danger my-1"></span>
            </div>

            <div class="form-group mt-3">
                <b><label class='control-label'>6. Los mandos de la puesta en marcha, aceleración, elevación y freno, deberán reunir las condiciones de seguridad necesarias para evitar el accionamiento involuntario</label></b>
                <div>
                    <div class="btn-group w-100 mt-1" role="group" aria-label="Basic radio toggle button group">
                        <input type="radio" class="btn-check" name="SeguridadMandos" value="Ok" id="SeguridadMandosok" autocomplete="off" checked>
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="SeguridadMandosok">Ok</label>

                        <input type="radio" class="btn-check" name="SeguridadMandos" value="No Ok" id="SeguridadMandosnook" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill mx-2 my-2 w-25 p-1" for="SeguridadMandosnook">No ok</label>

                        <input type="radio" class="btn-check" name="SeguridadMandos" value="No Aplica" id="SeguridadMandosnoaplica" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="SeguridadMandosnoaplica">No aplica</label>
                    </div>
                    <span asp-validation-for="SeguridadMandos" class="text-danger my-1"></span>
                </div>
                <span asp-validation-for="SeguridadMandos" class="text-danger my-1"></span>
            </div>

            <div class="form-group mt-3">
                <b><label class='control-label'>7. El asiento del conductor deberá estar diseñado ergonómicamente, poseer soporte lumbar adecuado, ser cómodo, regulable en profundidad y tener la capacidad de neutralizar en medida suficiente las vibraciones</label></b>
                <div>
                    <div class="btn-group w-100 mt-1" role="group" aria-label="Basic radio toggle button group">
                        <input type="radio" class="btn-check" name="SillaConductor" value="Ok" id="SillaConductorok" autocomplete="off" checked>
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="SillaConductorok">Ok</label>

                        <input type="radio" class="btn-check" name="SillaConductor" value="No Ok" id="SillaConductornook" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill mx-2 my-2 w-25 p-1" for="SillaConductornook">No ok</label>

                        <input type="radio" class="btn-check" name="SillaConductor" value="No Aplica" id="SillaConductornoaplica" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="SillaConductornoaplica">No aplica</label>
                    </div>
                    <span asp-validation-for="SillaConductor" class="text-danger my-1"></span>
                </div>
                <span asp-validation-for="SillaConductor" class="text-danger my-1"></span>
            </div>

            <div class="form-group mt-3">
                <b>
                    <label class='control-label'>8. El autoelevador deberá estar provisto de los siguientes elementos de seguridad</label>
                </b>
            </div>

            <div class="form-group mt-3">
                <label class='control-label'>8.1 Cinturón de seguridad</label>
                <div>
                    <div class="btn-group w-100 mt-1" role="group" aria-label="Basic radio toggle button group">
                        <input type="radio" class="btn-check" name="CinturonSeguridad" value="Ok" id="CinturonSeguridadok" autocomplete="off" checked>
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="CinturonSeguridadok">Ok</label>

                        <input type="radio" class="btn-check" name="CinturonSeguridad" value="No Ok" id="CinturonSeguridadnook" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill mx-2 my-2 w-25 p-1" for="CinturonSeguridadnook">No ok</label>

                        <input type="radio" class="btn-check" name="CinturonSeguridad" value="No Aplica" id="CinturonSeguridadnoaplica" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="CinturonSeguridadnoaplica">No aplica</label>
                    </div>
                    <span asp-validation-for="CinturonSeguridad" class="text-danger my-1"></span>
                </div>
                <span asp-validation-for="CinturonSeguridad" class="text-danger my-1"></span>
            </div>

            <div class="form-group mt-3">
                <label class='control-label'> 8.2  Luces de giro, balizas, posición y freno </label>

                <div>
                    <div class="btn-group w-100 mt-1" role="group" aria-label="Basic radio toggle button group">
                        <input type="radio" class="btn-check" name="LucesGiroFreno" value="Ok" id="LucesGiroFrenook" autocomplete="off" checked>
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="LucesGiroFrenook">Ok</label>

                        <input type="radio" class="btn-check" name="LucesGiroFreno" value="No Ok" id="LucesGiroFrenonook" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill mx-2 my-2 w-25 p-1" for="LucesGiroFrenonook">No ok</label>

                        <input type="radio" class="btn-check" name="LucesGiroFreno" value="No Aplica" id="LucesGiroFrenonoaplica" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="LucesGiroFrenonoaplica">No aplica</label>
                    </div>
                    <span asp-validation-for="LucesGiroFreno" class="text-danger my-1"></span>
                </div>
                <span asp-validation-for="LucesGiroFreno" class="text-danger my-1"></span>
            </div>

            <div class="form-group mt-3">
                <label class='control-label'>8.3 Luces de trabajo en aquellos casos donde la tarea que se realice con el autoelevador así lo requiera</label>
                <div>
                    <div class="btn-group w-100 mt-1" role="group" aria-label="Basic radio toggle button group">
                        <input type="radio" class="btn-check" name="LucesTrabajo" value="Ok" id="LucesTrabajook" autocomplete="off" checked>
                        <label class="btn btn-outline-primary rounded-pill my-2 w-25 p-1" for="LucesTrabajook">Ok</label>

                        <input type="radio" class="btn-check" name="LucesTrabajo" value="No Ok" id="LucesTrabajonook" autocomplete="off">
                        <label class="btn btn-outline-primary rounded-pill mx-2 my-2 w-25 p-1" for="LucesTrabajonook">No ok</label>

                        <input type="radio" class="btn-check" name="LucesTrabajo" value="No Aplica" id="LucesTrabajonoaplica" autocomplete="off">
